{
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#bec7ce",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#30344d",
					"name": "Body Text",
					"slug": "body-text"
				},
				{
					"color": "#9ea7b8",
					"name": "Section Background",
					"slug": "section-bg"
				},
				{
					"color": "#2c3243",
					"name": "Headings",
					"slug": "heading"
				},
				{
					"color": "#e1e7f0",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#bd1800",
					"name": "Secondary",
					"slug": "secondary"
				}
			]
		},
		"layout": {
			"contentSize": "999px",
			"wideSize": "1170px"
		},
		"spacing": {
			"units": [
				"px",
				"%",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Exo",
							"fontStretch": "",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/exo/Exo-Regular.ttf"
							]
						},
						{
							"fontFamily": "Exo",
							"fontStretch": "",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/exo/Exo-Medium.ttf"
							]
						},
						{
							"fontFamily": "Exo",
							"fontStretch": "",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/exo/Exo-SemiBold.ttf"
							]
						},
						{
							"fontFamily": "Exo",
							"fontStretch": "",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/exo/Exo-Bold.ttf"
							]
						}
					],
					"fontFamily": "\"Exo\", sans-serif",
					"name": "Exo",
					"slug": "exo"
				}
			],
			"fontSizes": [
				{
					"name": "Body Text",
					"size": "16px",
					"slug": "body-text"
				},
				{
					"name": "Extra Small",
					"size": "12px",
					"slug": "extra-small"
				},
				{
					"name": "Small",
					"size": "14px",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "18px",
					"slug": "medium"
				},
				{
					"name": "Regular",
					"size": "22px",
					"slug": "regular"
				},
				{
					"name": "Large",
					"size": "26px",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "32px",
					"slug": "extra-large"
				},
				{
					"name": "Big",
					"size": "clamp(1.75rem, 1.575rem + 0.875vw, 2.625rem)",
					"slug": "big"
				},
				{
					"name": "Huge",
					"size": "clamp(40px, calc(2.25rem + ((1vw - 3.2px) * 2.375)), 74px);",
					"slug": "huge"
				},
				{
					"name": "Gigantic",
					"size": "clamp(42px, calc(2.625rem + ((1vw - 3.2px) * 2.75)), 86px);",
					"slug": "gigantic"
				},
				{
					"name": "Banner Title",
					"size": "clamp(2.625rem, 2.219rem + 2.031vw, 4.25rem)",
					"slug": "banner-title"
				},
				{
					"name": "Section Title",
					"size": "clamp(2rem, 1.781rem + 1.094vw, 2.875rem)",
					"slug": "section-title"
				}
			],
			"fontWeight": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/navigation": {
				"color": {
					"text": "var(--wp--preset--color--secondary);"
				},
				"elements": {
					"link": {
						":active": {
							"color": {
								"text": "var(--wp--preset--color--primary);"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--primary);"
							}
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary);"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--secondary);"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--exo)",
					"fontSize": "15px",
					"fontWeight": "600",
					"letterSpacing": "2px",
					"textTransform": "capitalize"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary);"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--secondary)"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "clamp(2rem, 1.825rem + 0.875vw, 2.875rem)",
					"fontWeight": "700",
					"lineHeight": "1.2",
					"textTransform": "uppercase"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--body-text)"
		},
		"elements": {
			"button": {
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--white)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "40px",
					"lineHeight": "1.5"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "30px",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "24px"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "22px"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "18px"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "16px"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--heading)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--exo)",
					"fontWeight": "700"
				}
			},
			"link": {
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					},
					"outline": {
						"style": "dotted",
						"width": "1px"
					}
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--heading)"
				}
			}
		},
		"spacing": {
			"margin": {
				"bottom": "0",
				"left": "0",
				"right": "0",
				"top": "0"
			},
			"padding": {
				"bottom": "0",
				"left": "0",
				"right": "0",
				"top": "0"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--exo)",
			"fontSize": "var(--wp--preset--font-size--body-text)",
			"fontWeight": "500",
			"lineHeight": "1.5rem"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"title": "Lite",
	"version": 2
}